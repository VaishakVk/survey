{
	"$schema": "http://json-schema.org/draft-07/schema",
	"type": "object",
	"title": "Create Survey",
	"description": "Create Survey.",
	"additionalProperties": false,
	"required": ["title", "questions"],
	"properties": {
		"title": {
			"type": "string"
		},
		"questions": {
			"type": "array",
			"items": {
				"type": "object",
				"required": ["question"],
				"properties": {
					"question": {
						"type": "string"
					}
				}
			}
		}
	}
}
